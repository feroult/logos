<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>applogos</application>
    <version>${appengine.app.version}</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
        <property name="gapi.service.account.email" valeu="${gapi.service.account.email}" />
        <property name="gapi.service.account.key" valeu="/${gapi.service.account.key.file}" />
    </system-properties>
</appengine-web-app>
